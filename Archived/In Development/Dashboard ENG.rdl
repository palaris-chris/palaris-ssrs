    <DataSet Name="WonLostRatio">
      <Query>
        <DataSourceName>PalarisZoho</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BULabel">
            <Value>=Parameters!BULabel.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;
WITH BUWonLost AS
(SELECT
  Potentials.[Opportunity ID]
  ,Potentials.[POTENTIALID]
  ,Potentials.[Opportunity ID] AS OppLabel
  ,Potentials.[Potential Name]
  ,Potentials.[Created Time]
  ,Potentials.[Closing Date]
  ,Potentials.[Modified Time]
  ,Potentials.[Probability]
  ,CASE 
	WHEN Potentials.[Service] IS null THEN 'No Service Entered'
	ELSE Potentials.[Service]
END AS [Service]
  ,Potentials.[Business Unit]
  ,Potentials.[Amount]
  ,Potentials.[Amount] AS [PivotAmount]
  ,CASE 
	WHEN Potentials.Stage = 'Closed Lost' THEN 'BU Lost'
	WHEN Potentials.Stage = 'Closed Won' THEN 'BU Won'
	ELSE 'Still Open'
END AS "Ratios" 
  ,CASE 
	WHEN Potentials.Stage = 'Closed Lost' THEN 'BU Lost'
	WHEN Potentials.Stage = 'Closed Won' THEN 'BU Won'
	ELSE 'BU Still Open Amount'
END AS "Ratios2", 
Potentials.Stage,
@BULabel AS Label

FROM
  Potentials
WHERE Potentials.[Business Unit] = @BULabel
),

OtherWonLost AS
(SELECT
  Potentials.[Opportunity ID]
  ,Potentials.[POTENTIALID]
  ,Potentials.[Opportunity ID] AS OppLabel
  ,Potentials.[Potential Name]
  ,Potentials.[Created Time]
  ,Potentials.[Closing Date]
  ,Potentials.[Modified Time]
  ,Potentials.[Probability]  
,CASE 
	WHEN Potentials.[Service] IS null THEN 'No Service Entered'
	ELSE Potentials.[Service]
END AS [Service]
  ,Potentials.[Business Unit]
  ,Potentials.[Amount]
  ,Potentials.[Amount] AS [PivotAmount]
  ,CASE 
	WHEN Potentials.Stage = 'Closed Lost' THEN 'Lost'
	WHEN Potentials.Stage = 'Closed Won' THEN 'Won'
	ELSE 'Still Open'
END AS "Ratios" 
  ,CASE 
	WHEN Potentials.Stage = 'Closed Lost' THEN 'LostAmount'
	WHEN Potentials.Stage = 'Closed Won' THEN 'WonAmount'
	ELSE 'Still Open Amount'
END AS "Ratios2", 
Potentials.Stage,
'Other BUs' AS Label
FROM
  Potentials
WHERE Potentials.[Business Unit] &lt;&gt; @BULabel
),

Result AS
(SELECT * FROM BUWonLost 
UNION
SELECT * FROM OtherWonLost)

SELECT * FROM Result

PIVOT 
(Count(Result.[OppLabel]) for [Ratios] IN ([Lost], [Won], [Still Open]))
AS WonLost
PIVOT 
(Sum([PivotAmount]) for [Ratios2] IN ([LostAmount], [WonAmount], [Still Open Amount], [BU Lost], [BU Won], [BU Still Open Amount]))
AS Won

ORDER BY [Closing Date] asc</CommandText>
