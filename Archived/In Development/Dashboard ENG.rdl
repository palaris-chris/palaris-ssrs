    <DataSet Name="FullPipeline">
      <Query>
        <DataSourceName>PalarisZoho</DataSourceName>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;
SELECT
SUM(Potentials.[Amount]*(Potentials.[Probability]/100.0)) AS Value, Potentials.[Business Unit] AS Category, 'Pipeline' AS Label
FROM
  Potentials
WHERE Potentials.[Stage] NOT IN ('Closed Lost', 'Closed Won')
