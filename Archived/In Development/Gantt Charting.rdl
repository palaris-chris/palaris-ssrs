  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT      [Palaris$Job].No_ AS Task, [Palaris$Job].No_ AS PNo, [Palaris$Resource].[Name], [Palaris$Job].[Starting Date] AS StartDate, [Palaris$Job].[Ending Date] AS EndDate, [Palaris$Job].[Est_ Project Completion _]/100 AS PercentComplete, DATEDIFF(day, [Palaris$Job].[Starting Date], [Palaris$Job].[Ending Date]) * ([Palaris$Job].[Est_ Project Completion _]/100) AS CompletedDays, DATEDIFF(day, [Palaris$Job].[Starting Date], [Palaris$Job].[Ending Date]) * (1 - ([Palaris$Job].[Est_ Project Completion _]/100)) AS RemainingDays
FROM
  [Palaris$Job]
LEFT JOIN [Palaris$Resource]
ON [Palaris$Job].[Person Responsible]=[Palaris$Resource].[No_]
WHERE
  [Palaris$Job].[Project Status] = N'Active' 
  AND [Palaris$Job].[Ending Date] != '1/01/1753 12:00:00 AM'
ORDER BY [Palaris$Job].No_ ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
