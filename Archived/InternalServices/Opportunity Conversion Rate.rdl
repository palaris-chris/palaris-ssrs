  <DataSets>
    <DataSet Name="OppStageTime">
      <Query>
        <DataSourceName>PalarisZoho</DataSourceName>
        <CommandText>SELECT
  Potentials.[Opportunity ID]
  ,Potentials.[Created Time]
  ,Potentials.[Probability]
  ,Potentials.[Business Unit]
  ,CASE 
	WHEN Potentials.Stage = 'Closed Lost' THEN 'Lost Ratio'
	WHEN Potentials.Stage = 'Closed Won' THEN 'Won Ratio'
	ELSE 'Still Open'
END AS "Ratios", 
  CASE 
	WHEN Potentials.Stage = 'Closed Lost' THEN '2'
	WHEN Potentials.Stage = 'Closed Won' THEN '1'
	ELSE '3'
END AS "Ordering", 
Potentials.Stage

FROM
  Potentials</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Opportunity_ID">
          <DataField>Opportunity ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Created_Time">
          <DataField>Created Time</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Ratios">
          <DataField>Ratios</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Probability">
          <DataField>Probability</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Business_Unit">
          <DataField>Business Unit</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Ordering">
          <DataField>Ordering</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Stage">
          <DataField>Stage</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MonthYear">
          <Value>=Format(CDate(Fields!Created_Time.Value), "MMMM yy")</Value>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
