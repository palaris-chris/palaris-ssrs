  <DataSets>
    <DataSet Name="WIP">
      <Query>
        <DataSourceName>PalarisNAV</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@JobNo_">
            <Value>=Parameters!JobNo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;
WITH JobTasks AS
(
SELECT 
jt.[Job No_]
, jt.[Job Task No_]
, jt.Description AS Name
FROM dbo.[Palaris$Job Task] jt
WHERE jt.[Job No_] = @JobNo_
AND jt.[Job Task Type] = 0 
),

PELJobTasks AS
(
SELECT 
jt.[Job No_]
, jt.[Job Task No_]
, jt.Description AS Name
FROM dbo.[Palaris Europe Limited$Job Task] jt
WHERE jt.[Job No_] = @JobNo_
AND jt.[Job Task Type] = 0 
),

JPL AS 
(
SELECT 
jpl.[Job No_]
,job.[Global Dimension 2 Code] AS BU
,jpl.[Job Task No_]
,jt.[Name] AS [Task]
,CASE  WHEN jpl.[Line Type] = '0' THEN 'Schedule'
    WHEN jpl.[Line Type] = '1' THEN 'Contract'
    END AS [Line Type]
,jpl.No_ 
,jpl.[Planning Date] AS DATE
,CASE WHEN
jpl.[Qty_ to Invoice] &lt;&gt; 0 THEN DATEDIFF(day, jpl.[Planning Date], GETDATE())
END AS AgedDays
,CASE  WHEN jpl.Type = '2' THEN ac.[Name]
    WHEN jpl.Type = '0' THEN res.[Name]
    END AS Name
,CASE WHEN jpl.Type = '2' THEN 'G/L Account'
	WHEN jpl.No_ = 'R9998' THEN 'Milestone Allocation'
    WHEN jpl.Type = '0' THEN 'Resource'
    END AS Type
,jpl.[Job Position Title]    
,null AS [RevenueCategory]   
,jpl.Description
,jpl.[Document No_]
,jpl.Quantity
,jpl.[Quantity (Base)]
,jpl.[Location Code]
,jpl.[Work Type Code]
,jpl.[Unit of Measure Code] 
,jpl.[Unit Cost (LCY)] AS [Unit Cost]  
,jpl.[Total Cost (LCY)] AS [Total Cost]  
,jpl.[Unit Price (LCY)] AS [Unit Price]
,jpl.[Total Price (LCY)] AS [Total Price]

,jpl.[Line Amount (LCY)]

,CASE WHEN jpl.[Line Type] = '0' THEN null
WHEN jpl.[Line Type] = '1' THEN jpl.[Qty_ to Transfer to Invoice]
END AS [Qty_ to Transfer to Invoice]
,CASE WHEN jpl.[Line Type] = '0' THEN null
WHEN jpl.[Line Type] = '1' THEN jpl.[Qty_ to Invoice]
END AS [Qty_ to Invoice]
,CASE WHEN jpl.[Line Type] = '0' THEN null
    WHEN jpl.[Line Type] = '1' THEN (jpl.Quantity - jpl.[Qty_ to Invoice]) 
    END AS [Qty_ Invoiced]
FROM dbo.[Palaris$Job Planning Line] jpl


  LEFT JOIN [DynamicsNAVPalaris].dbo.[Palaris$Resource] res ON jpl.[No_] = res.[No_]  
  LEFT JOIN [DynamicsNAVPalaris].dbo.[Palaris$G_L Account] ac ON jpl.No_ = ac.No_  
  LEFT JOIN [DynamicsNAVPalaris].dbo.[Palaris$Job] job ON jpl.[Job No_] = job.No_ 
  LEFT JOIN JobTasks jt ON jpl.[Job Task No_] = jt.[Job Task No_]

WHERE jpl.[Job No_] = @JobNo_
AND jpl.[Line Type] = 1
),

JPL_PEL AS 
(
SELECT 
jpl.[Job No_]
,job.[Global Dimension 2 Code] AS BU
,jpl.[Job Task No_]
,jt.[Name] AS [Task]
,CASE  WHEN jpl.[Line Type] = '0' THEN 'Schedule'
    WHEN jpl.[Line Type] = '1' THEN 'Contract'
    END AS [Line Type]
,jpl.No_ 
,jpl.[Planning Date] AS DATE
,CASE WHEN
jpl.[Qty_ to Invoice] &lt;&gt; 0 THEN DATEDIFF(day, jpl.[Planning Date], GETDATE())
END AS AgedDays
,CASE  WHEN jpl.Type = '2' THEN ac.[Name]
    WHEN jpl.Type = '0' THEN res.[Name]
    END AS Name
,CASE WHEN jpl.Type = '2' THEN 'G/L Account'
	WHEN jpl.No_ = 'R9998' THEN 'Milestone Allocation'
    WHEN jpl.Type = '0' THEN 'Resource'
    END AS Type
,jpl.[Job Position Title]    
,null AS [RevenueCategory]   
,jpl.Description
,jpl.[Document No_]
,jpl.Quantity
,jpl.[Quantity (Base)]
,jpl.[Location Code]
,jpl.[Work Type Code]
,jpl.[Unit of Measure Code] 
,jpl.[Unit Cost (LCY)] AS [Unit Cost]  
,jpl.[Total Cost (LCY)] AS [Total Cost]  
,jpl.[Unit Price (LCY)] AS [Unit Price]
,jpl.[Total Price (LCY)] AS [Total Price]

,jpl.[Line Amount (LCY)]

,CASE WHEN jpl.[Line Type] = '0' THEN null
WHEN jpl.[Line Type] = '1' THEN jpl.[Qty_ to Transfer to Invoice]
END AS [Qty_ to Transfer to Invoice]
,CASE WHEN jpl.[Line Type] = '0' THEN null
WHEN jpl.[Line Type] = '1' THEN jpl.[Qty_ to Invoice]
END AS [Qty_ to Invoice]
,CASE WHEN jpl.[Line Type] = '0' THEN null
    WHEN jpl.[Line Type] = '1' THEN (jpl.Quantity - jpl.[Qty_ to Invoice]) 
    END AS [Qty_ Invoiced]
FROM dbo.[Palaris$Job Planning Line] jpl


  LEFT JOIN [DynamicsNAVPalaris].dbo.[Palaris Europe Limited$Resource] res ON jpl.[No_] = res.[No_]  
  LEFT JOIN [DynamicsNAVPalaris].dbo.[Palaris Europe Limited$G_L Account] ac ON jpl.No_ = ac.No_  
  LEFT JOIN [DynamicsNAVPalaris].dbo.[Palaris Europe Limited$Job] job ON jpl.[Job No_] = job.No_ 
  LEFT JOIN PELJobTasks jt ON jpl.[Job Task No_] = jt.[Job Task No_]

WHERE jpl.[Job No_] = @JobNo_
AND jpl.[Line Type] = 1
)

SELECT Date
,DATEDIFF(day, Date, GETDATE()) AS AgedDays
,SUM([Line Amount (LCY)]) AS WIP
FROM JPL
WHERE [Qty_ Invoiced] = 0
AND DATE &lt; GETDATE()
GROUP BY DATE

UNION

SELECT Date
,DATEDIFF(day, Date, GETDATE()) AS AgedDays
,SUM([Line Amount (LCY)]) AS WIP
FROM JPL_PEL
WHERE [Qty_ Invoiced] = 0
AND DATE &lt; GETDATE()
GROUP BY DATE</CommandText>
