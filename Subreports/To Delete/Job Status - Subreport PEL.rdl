  <DataSets>
    <DataSet Name="ActiveJobs">
      <Query>
        <DataSourceName>PalarisNAV</DataSourceName>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;
SELECT
  job.No_
  ,job.Description
  ,job.[Global Dimension 2 Code]
  ,job.[Job Type]
  ,job.[Person Responsible]
  ,res.[Windows Name]
  
FROM
 [Palaris Europe Limited$Job] job WITH (NOLOCK)
 LEFT JOIN [Palaris Europe Limited$Resource] res WITH (NOLOCK) ON job.[Person Responsible] = res.[No_]
WHERE
  job.[Project Status] = N'ACTIVE'
  /*AND res.[Windows Name] = @CurrentUser*/
ORDER BY job.No_</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="No_">
          <DataField>No_</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Global_Dimension_2_Code">
          <DataField>Global Dimension 2 Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Job_Type">
          <DataField>Job Type</DataField>
