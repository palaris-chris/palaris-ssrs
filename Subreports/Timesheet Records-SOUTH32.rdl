        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;
WITH BUGroup AS
(
SELECT [Business Unit Group], [Business Unit]
    FROM [Business Unit Group]
),

SUBCONTRCT_JPL AS (
      SELECT
        jpl.[Job No_],
		CONVERT(varchar(10),jpl.[Job Contract Entry No_]) AS [LineID],
  	job.[Global Dimension 2 Code] as [BU],
        jpl.[Job Task No_],
        jt.Description          AS [Task],
        job.[Description],
        ven.Name AS Vendor,
	'Subcontractor' AS Resource,
        jpl.[Job Task No_] AS [Vendor No_],
        jpl.No_,
        jpl.[Quantity (Base)],
        'Schedule' AS [Type],
	jpl.[Work Type Code],
        jpl.[Unit Cost (LCY)]   AS [Unit Cost],
        jpl.[Unit Price (LCY)]  AS [Unit Price],
        jpl.[Total Cost (LCY)]  AS [Total Cost],
        jpl.[Total Price (LCY)] AS [Total Price],
        jpl.[Line Amount (LCY)] AS [Line Amount]
		, jpl.[Quantity] AS ScheduleQty
		, jpl.[Total Cost (LCY)] AS ScheduleTotalCost
		, jpl.[Line Amount (LCY)] AS ScheduleTotalPrice
		, NULL AS UsageQty
		, NULL AS UsageTotalCost
		, NULL AS UsageTotalPrice
				
		
      FROM
        [Palaris$Job Planning Line] jpl

        LEFT JOIN [Palaris$Job] job on jpl.[Job No_] = job.[No_]
        LEFT JOIN [Palaris$Job Task] jt on (jpl.[Job Task No_] = jt.[Job Task No_] AND jpl.[Job No_] = jt.[Job No_])

        LEFT JOIN [Palaris$Vendor] ven on jpl.[Job Task No_] = ven.No_
      WHERE jpl.[Work Type Code] NOT LIKE 'OSA%'
            AND jpl.[Work Type Code] NOT LIKE 'KM%'
            AND jpl.[Work Type Code] NOT LIKE 'PAY%'
            AND jpl.[No_] IN ('51150', '51160')
			AND jpl.[Line Type] = 0
	    AND jpl.[Job No_] = @JobNo_
  ),

  SUBCONSULT_JPL AS (
      SELECT
        jpl.[Job No_],
		CONVERT(varchar(10),jpl.[Job Contract Entry No_]) AS [LineID],
  	job.[Global Dimension 2 Code] as [BU],
        jpl.[Job Task No_],
        jt.Description          AS [Task],
        job.[Description],
        ven.Name AS Vendor,
	res.Name AS Resource,
        res.[Vendor No_],
        jpl.No_,
        jpl.[Quantity (Base)],
        'Schedule' AS [Type],
	jpl.[Work Type Code],
        jpl.[Unit Cost (LCY)]   AS [Unit Cost],
        jpl.[Unit Price (LCY)]  AS [Unit Price],
        jpl.[Total Cost (LCY)]  AS [Total Cost],
        jpl.[Total Price (LCY)] AS [Total Price],
        jpl.[Line Amount (LCY)] AS [Line Amount]
		, jpl.[Quantity] AS ScheduleQty
		, jpl.[Total Cost (LCY)] AS ScheduleTotalCost
		, jpl.[Line Amount (LCY)] AS ScheduleTotalPrice
		, NULL AS UsageQty
		, NULL AS UsageTotalCost
		, NULL AS UsageTotalPrice
      FROM
        [Palaris$Job Planning Line] jpl

        LEFT JOIN [Palaris$Job] job on jpl.[Job No_] = job.[No_]
        LEFT JOIN [Palaris$Job Task] jt on (jpl.[Job Task No_] = jt.[Job Task No_] AND jpl.[Job No_] = jt.[Job No_])
        LEFT JOIN [Palaris$Resource] res on jpl.No_ = res.No_
        LEFT JOIN [Palaris$Vendor] ven on res.[Vendor No_] = ven.No_
      WHERE jpl.[Work Type Code] NOT LIKE 'OSA%'
            AND jpl.[Work Type Code] NOT LIKE 'KM%'
            AND jpl.[Work Type Code] NOT LIKE 'PAY%'
            AND (LEFT(jpl.No_, 2) = 'R9' AND jpl.No_ &lt;&gt; 'R9998')
			AND jpl.[Line Type] = 0
            AND jpl.[Job No_] = @JobNo_
  ),

  SUBCONTRCT_JLE AS (
      SELECT
        jle.[Job No_],
		jle.[Entry No_] AS [LineID],
  	job.[Global Dimension 2 Code] as [BU],
        jle.[Job Task No_],
        jt.Description          AS [Task],
        job.[Description],        
	ven.Name AS Vendor,
	'Subcontractor' AS Resource,
        jle.[Job Task No_] AS [Vendor No_],
        jle.No_,
        jle.[Quantity (Base)],
        'Usage' AS [Type],
	jle.[Work Type Code],
        jle.[Unit Cost (LCY)]   AS [Unit Cost],
        jle.[Unit Price (LCY)]  AS [Unit Price],
        jle.[Total Cost (LCY)]  AS [Total Cost],
        jle.[Total Price (LCY)] AS [Total Price],
        jle.[Line Amount (LCY)] AS [Line Amount]
		, NULL AS ScheduleQty
		, NULL AS ScheduleTotalCost
		, NULL AS ScheduleTotalPrice
		, jle.[Quantity] AS UsageQty
		, jle.[Total Cost (LCY)] AS UsageTotalCost
		, jle.[Line Amount (LCY)] AS UsageTotalPrice
      FROM
        [Palaris$Job Ledger Entry] jle

        LEFT JOIN [Palaris$Job] job on jle.[Job No_] = job.[No_]
        LEFT JOIN [Palaris$Job Task] jt on (jle.[Job Task No_] = jt.[Job Task No_] AND jle.[Job No_] = jt.[Job No_])

        LEFT JOIN [Palaris$Vendor] ven on jle.[Job Task No_] = ven.No_
      WHERE jle.[Work Type Code] NOT LIKE 'OSA%'
            AND jle.[Work Type Code] NOT LIKE 'KM%'
            AND jle.[Work Type Code] NOT LIKE 'PAY%'
            AND jle.[No_] IN ('51150', '51160')
				AND jle.[Job No_] = @JobNo_
 	AND jle.[Entry Type] = 0
  ),

  SUBCONSULT_JlE AS (
      SELECT
        jle.[Job No_],
		jle.[Entry No_] AS [LineID],
  	job.[Global Dimension 2 Code] as [BU],
        jle.[Job Task No_],
        jt.Description          AS [Task],
        job.[Description],
        ven.Name AS Vendor,
	res.Name AS Resource,
        res.[Vendor No_],
        jle.No_,
        jle.[Quantity (Base)],
        'Usage' AS [Type],
	jle.[Work Type Code],
        jle.[Unit Cost (LCY)]   AS [Unit Cost],
        jle.[Unit Price (LCY)]  AS [Unit Price],
        jle.[Total Cost (LCY)]  AS [Total Cost],
        jle.[Total Price (LCY)] AS [Total Price],
        jle.[Line Amount (LCY)] AS [Line Amount]
		, NULL AS ScheduleQty
		, NULL AS ScheduleTotalCost
		, NULL AS ScheduleTotalPrice
		, jle.[Quantity] AS UsageQty
		, jle.[Total Cost (LCY)] AS UsageTotalCost
		, jle.[Line Amount (LCY)] AS UsageTotalPrice
      FROM
        [Palaris$Job Ledger Entry] jle

        LEFT JOIN [Palaris$Job] job on jle.[Job No_] = job.[No_]
        LEFT JOIN [Palaris$Job Task] jt on (jle.[Job Task No_] = jt.[Job Task No_] AND jle.[Job No_] = jt.[Job No_])
        LEFT JOIN [Palaris$Resource] res on jle.No_ = res.No_
        LEFT JOIN [Palaris$Vendor] ven on res.[Vendor No_] = ven.No_
      WHERE jle.[Work Type Code] NOT LIKE 'OSA%'
            AND jle.[Work Type Code] NOT LIKE 'KM%'
            AND jle.[Work Type Code] NOT LIKE 'PAY%'
            AND (LEFT(jle.No_, 2) = 'R9' AND jle.No_ &lt;&gt; 'R9998')
            AND jle.[Job No_] = @JobNo_
  AND jle.[Entry Type] = 0
  ),

JLE_JPL AS (
    SELECT *
    FROM SUBCONTRCT_JPL
    UNION
    SELECT *
    FROM SUBCONSULT_JPL
    UNION
    SELECT *
    FROM SUBCONTRCT_JLE
    UNION
    SELECT *
    FROM SUBCONSULT_JLE
  )

SELECT * FROM JLE_JPL</CommandText>
