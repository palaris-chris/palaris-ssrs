        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;
WITH JLE AS
(
SELECT
  jle.[Job No_]
  ,jle.[Job Task No_]
  ,jle.[Entry No_] as [Line_No_]
  ,jle.[Posting Date] as [Date]
  ,jle.Type
  ,jle.No_
  ,jle.Description
  ,jle.Quantity AS [Qty]
  ,jle.[Quantity (Base)] 
  ,jle.[Total Cost (LCY)] AS [Cost]
  ,jle.[Total Price (LCY)]
  ,jle.[Line Discount _]
  ,jle.[Line Discount Amount (LCY)]
  ,jle.[Line Amount (LCY)] AS [Price]
  ,jle.[Unit of Measure Code] AS [Unit]
  ,jle.[Work Type Code] AS [Work Type]
  ,jle.[Global Dimension 2 Code] as [BU]
  ,jle.[Entry Type] as [Line Type]
, NULL AS ScheduleQty
, NULL AS ScheduleTotalCost
, NULL AS ScheduleTotalPrice
, jle.[Quantity] AS UsageQty
, jle.[Total Cost (LCY)] AS UsageTotalCost
, jle.[Line Amount (LCY)] AS UsageTotalPrice
  ,res.[Name] as [Resource]
  ,res.[Search Name]
  ,jt.[Description] as [Task]
FROM
  [Palaris$Job Ledger Entry] jle
 LEFT JOIN [Palaris$Resource] res WITH (NOLOCK) ON jle.[No_] = res.[No_]
 LEFT JOIN [Palaris$Job Task] jt WITH (NOLOCK) ON jle.[Job Task No_] = jt.[Job Task No_] AND jle.[Job No_] = jt.[Job No_]

WHERE
  jle.[Job No_] = @JobNo_
  AND jle.[Entry Type] = 0
  ),
  
  JPL AS 
  (
  SELECT
  jpl.[Job No_]
  ,jpl.[Job Task No_]
  ,jpl.[Line No_]
  ,jpl.[Planning Date] as [Date]
  ,jpl.Type
  ,jpl.No_
  ,jpl.Description
  ,jpl.Quantity AS [Qty]
  ,jpl.[Quantity (Base)]
  ,jpl.[Total Cost (LCY)] AS [Cost]
  ,jpl.[Total Price (LCY)]
  ,jpl.[Line Discount _]
  ,jpl.[Line Discount Amount (LCY)]
  ,jpl.[Line Amount (LCY)] AS [Price]
  ,jpl.[Unit of Measure Code] AS [Unit]
  ,jpl.[Work Type Code] AS [Work Type]
  ,res.[Global Dimension 2 Code] as [BU]
  ,jpl.[Schedule Line] AS [Line Type]
, jpl.[Quantity] AS ScheduleQty
, jpl.[Total Cost (LCY)] AS ScheduleTotalCost
, jpl.[Line Amount (LCY)] AS ScheduleTotalPrice
, NULL AS UsageQty
, NULL AS UsageTotalCost
, NULL AS UsageTotalPrice

  ,res.[Name] as [Resource]
  ,res.[Search Name]
  ,jt.[Description] as [Task]
FROM
  [Palaris$Job Planning Line] jpl
  LEFT JOIN [Palaris$Resource] res WITH (NOLOCK) ON jpl.[No_] = res.[No_]
  LEFT JOIN [Palaris$Job Task] jt WITH (NOLOCK) ON jpl.[Job Task No_] = jt.[Job Task No_] AND jpl.[Job No_] = jt.[Job No_]

WHERE
  jpl.[Job No_] = @JobNo_
  AND jpl.[Schedule Line] = 1
  )
  
  SELECT JPL.* FROM JPL
  UNION ALL
  SELECT JLE.* FROM JLE
  ORDER BY [Line Type] desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
